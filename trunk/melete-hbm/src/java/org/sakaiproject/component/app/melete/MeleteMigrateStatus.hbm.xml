<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.sakaiproject.component.app.melete.MeleteMigrateStatus" table="melete_migrate_status">
     <id name="startFlag" column="START_FLAG" type="int" unsaved-value="null">
      <generator class="assigned"/>
    </id>
     <property name="completeFlag" column="COMPLETE_FLAG" type="int" length="1" />
</class>
</hibernate-mapping>


